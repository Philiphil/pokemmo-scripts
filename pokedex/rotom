	function data_table_call() {
		$('main').find('#datatable').DataTable( {
			"dom": '<"recherche-table"f><"conteneur_tableau"t>',
			"paging": false, 
			"aaSorting": []
		}) ;

	};

	data_table_call() ;



	$('main').on('click','.switch-attaque', function(e){
		e.preventDefault();

		id_methode = $(this).val() ;
		id_pokemon = 479 ;
		oeuf = '11' ;
		prevo = '479' ;


		$.ajax({
			type : 'POST', 
			url : 'fiche-pokemon/index-attaques-chargement.php?id_methode=' 
			+ id_methode + '&id_pokemon=' + id_pokemon + '&oeuf=' + oeuf + '&sav_prevo=' + prevo    , 
			success : function(data){
				$('#load-attaque').html(data);
				$('.pop-up').css('display', 'none') ;	
				$('html').removeClass('padding-pop') ;

// $('main').find('#datatable').DataTable();
data_table_call() ;


}

});




	})





